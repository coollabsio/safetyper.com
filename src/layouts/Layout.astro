    <Footer />

    <script>
      const mobileMenuButton = document.getElementById("mobile-menu-button");
      const mobileMenu = document.getElementById("mobile-menu");

      function toggleMobileMenu() {
        mobileMenu?.classList.toggle("hidden");
        mobileMenu?.classList.toggle("flex");
      }

      mobileMenuButton?.addEventListener("click", toggleMobileMenu);

      mobileMenu?.querySelectorAll("a").forEach((link) => {
        link.addEventListener("click", () => {
          mobileMenu?.classList.add("hidden");
          mobileMenu?.classList.remove("flex");
        });
      });
    </script>

    <script>
      fetch("https://cdn-new.coollabs.io/business.json")
        .then((res) => res.json())
        .then((data) => {
          const el = document.getElementById("community-count");
          const elMobile = document.getElementById("community-count-mobile");
          if (data.discord) {
            const text = `(${(data.discord / 1000).toFixed(0)}k+)`;
            if (el) el.textContent = text;
            if (elMobile) elMobile.textContent = text;
          }
        })
        .catch(() => {});
    </script>
